#include<stdio.h>
int main()
{
    int no;
    printf("\n enter day no between 1to7:");
    scanf("%d",&no);
    switch(no)
    {
        cases: 1,2,3,4,5,6,7;
        printf("\n sunday,monday,tuesday,wednesday,thursday,friday,saturday");
        break;
        default:
        printf("\n please enter proper input");
        break;
    }
    return 0;
}

